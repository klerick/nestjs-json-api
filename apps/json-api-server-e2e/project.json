{
  "name": "json-api-server-e2e",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "implicitDependencies": ["json-api-server", "json-api-nestjs-sdk"],
  "targets": {
    "e2e": {
      "dependsOn": ["json-api-server:build", "json-api-nestjs-sdk:build"],
      "executor": "@nx/vitest:test",
      "outputs": ["{workspaceRoot}/coverage/{e2eProjectRoot}"],
      "options": {
        "config": "apps/json-api-server-e2e/vite.config.mts",
        "passWithNoTests": true,
        "reportsDirectory": "../../coverage/apps/json-api-server-e2e"
      }
    },
    "e2e-micro": {
      "dependsOn": ["json-api-server:build", "json-api-nestjs-sdk:build"],
      "executor": "@nx/vitest:test",
      "outputs": ["{workspaceRoot}/coverage/{e2eProjectRoot}"],
      "options": {
        "config": "apps/json-api-server-e2e/vite.config.mts",
        "passWithNoTests": true,
        "reportsDirectory": "../../coverage/apps/json-api-server-e2e"
      }
    }
  }
}
